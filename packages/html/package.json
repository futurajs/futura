{
  "name": "@futura/html",
  "version": "0.4.0",
  "description": "Virtual DOM based HTML implementation for futura",
  "keywords": [
    "elm",
    "tea",
    "flux",
    "fsm",
    "redux",
    "state",
    "state-machine",
    "ui",
    "virtual-dom"
  ],
  "author": "Ali Sabil <ali.sabil@gmail.com>",
  "homepage": "https://github.com/futurajs/futura",
  "license": "MIT",
  "main": "lib.cjs/index.js",
  "module": "lib.esm/index.js",
  "types": "types/index.d.ts",
  "files": [
    "lib.cjs",
    "lib.esm",
    "types"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/futurajs/futura.git"
  },
  "scripts": {
    "prepare": "npm run build",
    "prebuild": "npx rimraf lib.cjs/ lib.esm/ types/",
    "build": "npm run build:cjs && npm run build:esm && npm run build:types",
    "build:cjs": "npx tsc --target es5 --module commonjs --outDir lib.cjs",
    "build:esm": "npx tsc --target es5 --module es2015 --outDir lib.esm",
    "build:types": "npx tsc --declaration --emitDeclarationOnly --outDir types",
    "test": "npm test"
  },
  "bugs": {
    "url": "https://github.com/futurajs/futura/issues"
  },
  "dependencies": {
    "@futura/virtual-dom": "^0.4.0"
  },
  "devDependencies": {
    "rimraf": "^2.6.2",
    "tslint": "^5.11.0",
    "tslint-language-service": "angelozerr/tslint-language-service#3c6e13e7ee219968af2df6130fd8b57742c9ae74",
    "typedoc": "^0.12.0",
    "typescript": "^3.1.1"
  }
}
