{
  "compilerOptions": {
    "declaration": true,
    "declarationDir": "../dist/typings",
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": false,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "outDir": "../dist",
    "strictNullChecks": false,
    "suppressImplicitAnyIndexErrors": true,
    "target": "es5"
  }
}
